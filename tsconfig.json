{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/screens":["app/screens"],
      "@/screens/HomeScreen":["app/screens/HomeScreen"],
      "@/screens/NoteTakingScreen":["app/screens/NoteTakingScreen"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
